      ".clj"
      ".cljc"
      ".cljs"
      ".edn"
      ".el"
      ".lfe"
      ".lisp"
      ".lsp"
      ".rkt"
      ".scm"
    ]
  "prettier-atom":
    formatOnSaveOptions:
      enabled: true
      showInStatusBar: true
    prettierEslintOptions:
      prettierLast: true
    prettierOptions:
      bracketSpacing: false
      semi: false
      singleQuote: true
    useEslint: true
  "proto-repl":
    autoPrettyPrint: true
    displayExecutedCodeInRepl: false
  "python-black":
    lineLength: 80
  "tree-view":
    hideIgnoredNames: true
    squashDirectoryNames: true
  welcome:
    showOnStartup: false
".elm.source":

  "linter-python":
    executablePath: "/Users/dmotzenbecker/homebrew/bin/pylama"
    lintTrigger: "File modified"
    withPep8: true
  "linter-ui-default":
    alwaysTakeMinimumSpace: true
    gutterPosition: "Left"
    panelHeight: 326
    showPanel: true
    showProviderName: true
    statusBarRepresents: "Current File"
  "lisp-paredit": {}
  "omnisharp-atom":
    codeFormat: false
  parinfer:
    "file-extensions": [
